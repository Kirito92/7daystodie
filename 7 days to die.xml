<?xml version="1.0"?>
<Container version="2">
  <Name>7 days to die</Name>
  <Repository>Kirito92/7daystodie</Repository>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>26900</HostPort>
        <ContainerPort>26900</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/7 Days to Die</HostDir>
      <ContainerDir>/serverdata/7 Days to Die</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>UID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>GID</Name>
      <Mode/>
    </Variable>
  </Environment>
</Container>
